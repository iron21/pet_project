<h1>Editing contribute</h1>

<% form_for(@contribute) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :file_name %><br />
    <%= f.text_field :file_name %>
  </p>
  <p>
    <%= f.label :file_size %><br />
    <%= f.text_field :file_size %>
  </p>
  <p>
    <%= f.submit "Update" %>
  </p>
<% end %>

<%= link_to 'Show', @contribute %> |
<%= link_to 'Back', contributes_path %>
